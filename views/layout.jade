  body(cz-shortcut-listen='true')
    #wrap
      #superior
        ul#menu
          li
            a#logo(href='/productos/tableros#')  
          li
            a#inicio.texto-menu(href='/') inicio
          li
            a#empresa.texto-menu(href='/empresa') empresa
          li#productos-item
            a#productos.texto-menu(href='/productos') productos

              ul#productos-lista
                li.first
                  a.menu-desplegable(href='/productos/chapilla') chapilla
                li
                  a.menu-desplegable(href='/productos/cantos') cantos
                li.last
                  a.menu-desplegable(href='/productos/tableros') tableros
              .centro &nbsp;
              .bottom &nbsp;
              .seleccionado.off &nbsp;
          li
            a#contacto.texto-menu(href='/contacto') contáctenos
          li
            a#beneficios.texto-menu(href='/beneficios') beneficios

      != body

